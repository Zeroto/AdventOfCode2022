// A X == Rock
// B Y == Paper
// C Z == Scissors

let Scoring = 
  [
    ("A Y", 6+2)
    ("B Z", 6+3)
    ("C X", 6+1)
    ("A X", 3+1)
    ("B Y", 3+2)
    ("C Z", 3+3)
    ("A Z", 3)
    ("B X", 1)
    ("C Y", 2)
  ]
  |> Map.ofList

let part1 =
  input 
  |> fun s -> s.Split('\n')
  |> Array.choose (fun s -> Map.tryFind s Scoring)
  |> Array.sum

let Scoring2 = 
  [
    ("A Y", 3+1) //draw
    ("B Z", 6+3) //win
    ("C X", 2) //lose
    ("A X", 3)
    ("B Y", 3+2) //draw
    ("C Z", 6+1)
    ("A Z", 6+2)
    ("B X", 1)
    ("C Y", 3+3) //draw
  ]
  |> Map.ofList

let part2 =
  input 
  |> fun s -> s.Split('\n')
  |> Array.choose (fun s -> Map.tryFind s Scoring2)
  |> Array.sum